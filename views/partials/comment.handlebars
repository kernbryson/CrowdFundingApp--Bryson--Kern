

{{#each this}}
  <div class='row'>
    <div class='card'>
      <div class='card-body'>
        <p class='card-text'>{{body}}</p>
      </div>
      <div class='card-footer text-muted'>
        Posted by
        {{user.username}}
        on
        {{format_date createdAt}}
      </div>
    </div>
  </div>
{{/each}}